<Window x:Class="VK.ITMO.WPF.L21.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VK.ITMO.WPF.L21"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel HorizontalAlignment="Left" Height="100" Margin="35,60,0,0" VerticalAlignment="Top" Width="100" ButtonBase.Click="StackPanel_Click">
            <Button Name="butAdd" Content="Add"/>
            <Button Name="butMult" Content="Mult"/>
        </StackPanel>
        <TextBox Name="txtBox" HorizontalAlignment="Left" Height="23" Margin="35,22,0,0" TextWrapping="Wrap" Text="5" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
